{
  "name": "ac-timeconverter",
  "version": "0.0.1",
  "description": "Converts timestamps to seconds and vice versa",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "test-jenkins": "JUNIT_REPORT_PATH=./report.xml mocha --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_name='TimeConverter'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmpro/ac-timeconverter.git"
  },
  "keywords": [
    "seconds",
    "time",
    "timestamp"
  ],
  "author": "Mark Poepping",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmpro/ac-timeconverter/issues"
  },
  "homepage": "https://github.com/mmpro/ac-timeconverter#readme",
  "devDependencies": {
    "ac-conventional-changelog-template": "github:mmpro/ac-conventional-changelog-template",
    "corp-semantic-release": "github:mmpro/corp-semantic-release",
    "cz-customizable": "github:mmpro/cz-customizable",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^24.8.0",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "github:mmpro/mocha-jenkins-reporter",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-multi-reporters": "^1.1.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".czConfig.js"
    }
  }
}
